{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "defaultDestination": "wwwroot/lib/",
  "libraries": [    
    {
      "library": "bootstrap@5.0.0-beta1",
      "files": [ "dist/css/bootstrap.min.css", "dist/js/bootstrap.bundle.min.js" ],
      "destination": "wwwroot/lib/bootstrap"
    },
    {
      "library": "jcrop@3.0.1",
      "files": [ "dist/jcrop.js", "dist/jcrop.css" ],
      "destination": "wwwroot/lib/jcrop"
    },
    {
      "library": "js-year-calendar@1.0.2",
      "files": [ "dist/js-year-calendar.min.css", "dist/js-year-calendar.min.js" ],
      "destination": "wwwroot/lib/calendar"
    },
    {
      "library": "quill@1.3.7",
      "files": ["dist/*"],
      "destination": "wwwroot/lib/quilljs"
    }
  ]
}